[Unit]
Description=Filebrowser Service
After=network.target

[Service]
User=filebrowser
Group=torrenting
Type=simple
KillMode=process
ExecStart=/usr/bin/env filebrowser --config /etc/filebrowser/filebrowser.yaml --database /data/filebrowser.db
Restart=on-failure
RestartSec=3

[Install]
WantedBy=multi-user.target
